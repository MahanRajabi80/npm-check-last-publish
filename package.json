{
  "name": "npm-check-last-publish",
  "version": "0.1.1",
  "description": "check package.json dependency last publish time",
  "main": "./dist/src/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "build": "tsc",
    "publish": "np",
    "prepublish": "npm run build",
    "test": ""
  },
  "bin": "./dist/src/index.js",
  "files": [
    "index.js",
    "src/**/*.ts",
    "dist/**/*.js",
    "README.md"
  ],
  "keywords": [
    "last-publish",
    "check-publish",
    "package.json",
    "npm",
    "publish"
  ],
  "author": "Mahdi Sheibak",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "20.14.9",
    "chalk": "5.3.0",
    "date-fns": "3.6.0",
    "np": "10.0.6",
    "ora": "8.0.1",
    "tsx": "4.15.8",
    "typescript": "5.5.2"
  },
  "volta": {
    "node": "20.15.0",
    "npm": "10.8.1"
  }
}
